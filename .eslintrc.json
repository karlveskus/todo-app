{
  "extends": "airbnb-base",
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "describe": true,
    "it": true,
    "beforeEach": true
  },
  "rules": {
    "no-var": 0,
    "prefer-const": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0,
    "no-shadow": 0,
    "prefer-arrow-callback": 0,
    "no-use-before-define": 0
  }
}